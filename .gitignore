import java.util.Scanner;

public class combi {
    public static void main(String[] args) {
        int n,r;

       // C(n,r) = n! / (r! * (n-r)!)
        Scanner ınput=new Scanner(System.in);
        System.out.print("1.SAYI:");
        n= ınput.nextInt();
        System.out.print("2.SAYI:");
        r= ınput.nextInt();
double total=1;
    //n!
        for (int i=1;i<=n;i++){
         total*=i;
        }
        double total1=1;
        //r!
        for (int j=1;j<=r;j++){
            total1*=j;

        }
        double total2=1;
        //k=(n-r)
        int k=(n-r);
        for (int I=1;I<=k;I++){
            total2*=I;
           // System.out.print(total2);
        }
      double  c=total/(total1*total2);

        System.out.print(c);




    }}



